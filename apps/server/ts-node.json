{
  "ts-node": {
    "transpileOnly": true,
    "files": true,
    "compilerOptions": {
      "target": "ES5",
      "module": "commonjs",
      "outDir": "./lib",
      "downlevelIteration": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    }
  }
}